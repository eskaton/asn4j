TestEnumeration1 ::= ENUMERATED {
        a, b, c, ..., d(4), e, f
}

TestEnumeration2 ::= TestEnumeration1 ((c | d | e) ^ (d | e | f))
TestEnumeration3 ::= TestEnumeration1 (ALL EXCEPT (c | d))
