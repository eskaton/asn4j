X680-32 DEFINITIONS ::=
BEGIN

iso INTEGER ::= 1

identified-organization INTEGER ::= 3

dod INTEGER ::= 6

internet INTEGER ::= 1

ID ::= OBJECT IDENTIFIER

OID ::= ID

testObjectIdentifier1 OBJECT IDENTIFIER ::= { iso identified-organization dod internet }

testObjectIdentifier2 OBJECT IDENTIFIER ::= { iso(1) identified-organization(3) dod(6) internet(1) }

testObjectIdentifier3 OBJECT IDENTIFIER ::= { 1 3 6 1 }

testObjectIdentifier4 OBJECT IDENTIFIER ::= { testObjectIdentifier1 23 }

testObjectIdentifier5 ID ::= { iso 3 6 1 }

testObjectIdentifier6 OID ::= { iso 3 6 2 }

TestObjectIdentifiers ::= SEQUENCE {
    testObjectIdentifier1 OBJECT IDENTIFIER DEFAULT testObjectIdentifier1,
    testObjectIdentifier2 OBJECT IDENTIFIER DEFAULT testObjectIdentifier2,
    testObjectIdentifier3 OBJECT IDENTIFIER DEFAULT testObjectIdentifier3,
    testObjectIdentifier4 OBJECT IDENTIFIER DEFAULT testObjectIdentifier4,
    testObjectIdentifier5 OBJECT IDENTIFIER DEFAULT testObjectIdentifier5,
    testObjectIdentifier6 OBJECT IDENTIFIER DEFAULT testObjectIdentifier6
}

-- Test Root arcs according to X.660 A.2
testOidItuT OBJECT IDENTIFIER ::= { itu-t test(12) }

testOidCcitt OBJECT IDENTIFIER ::= { ccitt test(12) }

testOidIso OBJECT IDENTIFIER ::= { iso test(12) }

testOidJointIsoItuT OBJECT IDENTIFIER ::= { joint-iso-itu-t test(12) }

testOidJointIsoCcitt OBJECT IDENTIFIER ::= { joint-iso-ccitt test(12) }

TestObjectIdentifierRootArcs ::= SEQUENCE {
   testOidItuT           OBJECT IDENTIFIER DEFAULT testOidItuT,
   testOidCcitt          OBJECT IDENTIFIER DEFAULT testOidCcitt,
   testOidIso            OBJECT IDENTIFIER DEFAULT testOidIso,
   testOidJointIsoItuT   OBJECT IDENTIFIER DEFAULT testOidJointIsoItuT,
   testOidJointIsoCcitt  OBJECT IDENTIFIER DEFAULT testOidJointIsoCcitt
}

END
